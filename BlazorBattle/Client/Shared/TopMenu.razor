@inject IBananaService BananaService
@implements IDisposable

<AuthorizeView>
    <Authorized>
        <div class="top-menu">
            <span class="popover-header">
                <img src="icons/banana.png" />
                @BananaService.Bananas
            </span>
            <AddBananas />
        </div>
    </Authorized>
    <NotAuthorized>
        <div>
            <NavLink href="">Login</NavLink> |
            <NavLink href="register">Register</NavLink>
        </div>
    </NotAuthorized>
</AuthorizeView>

